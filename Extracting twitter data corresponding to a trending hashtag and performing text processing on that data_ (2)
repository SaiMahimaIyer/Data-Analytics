      "('AI', '#', 'Arti\\\\xe2\\\\x80\\\\xa6')\n",
      "('#', 'Arti\\\\xe2\\\\x80\\\\xa6', \"'\")\n"
     ]
    }
   ],
   "source": [
    "n = 3\n",
    "threegrams = ngrams(words, n)\n",
    "\n",
    "for grams in threegrams:\n",
    "  print(grams)"
   ]
  },
